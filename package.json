{
  "name": "apostrophe-staging",
  "version": "1.0.0",
  "description": "Apostrophe Staging Site",
  "main": "app.js",
  "scripts": {
    "start": "node app.js"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "P'unk Avenue",
  "dependencies": {
